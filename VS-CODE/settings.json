{   

    "code-runner.executorMap": {

        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt.exe -Dvelox && $dir$fileNameWithoutExt.exe", 


        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt.exe && $dir$fileNameWithoutExt.exe",
        "python": "cd $dir && python -u $fileName" 
    },
    "cph.language.cpp.SubmissionCompiler": "GNU G++20 13.2 (64 bit, winlibs)",
    "editor.accessibilitySupport": "off",
    "workbench.layoutControl.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 5,
    "editor.minimap.sectionHeaderFontSize": 5,
    "zenMode.hideLineNumbers": false,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": false,
    "editor.lineHeight": 1.2,
    "workbench.activityBar.location": "top",
    "editor.fontSize": 16.5,
    // "terminal.integrated.tabs.defaultColor": "terminal.ansiGreen",
    "terminal.integrated.fontSize": 16,
    "code-runner.runInTerminal": true,
    "workbench.colorTheme": "Mariana Dark Theme",
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -Dvelox -Wl,--stack=268435456",
    "cph.language.cpp.Args": "-Dvelox",
    "explorer.confirmDelete": false,
    "workbench.settings.applyToAllProfiles": [

    ],
    "lldb.suppressUpdateNotifications": true,
    "tabnine.experimentalAutoImports": true,
    "explorer.confirmPasteNative": false,
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "on"

}